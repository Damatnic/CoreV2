# Astral Core Environment Variables
# Copy this file to .env and fill in your values

# Auth0 Configuration
VITE_AUTH0_DOMAIN=your-auth0-domain.auth0.com
VITE_AUTH0_CLIENT_ID=your-auth0-client-id
VITE_AUTH0_CLIENT_SECRET=your-auth0-client-secret
VITE_AUTH0_CALLBACK_URL=http://localhost:5000/callback

# API Configuration
VITE_API_BASE_URL=http://localhost:3000/api
VITE_WEBSOCKET_URL=ws://localhost:3000

# Push Notifications (VAPID Keys)
VITE_VAPID_PUBLIC_KEY=your-vapid-public-key
VITE_VAPID_PRIVATE_KEY=your-vapid-private-key

# Sentry Error Tracking (Optional)
VITE_SENTRY_DSN=your-sentry-dsn

# Feature Flags
VITE_ENABLE_AI_CHAT=true
VITE_ENABLE_VIDEO_CHAT=true
VITE_ENABLE_PUSH_NOTIFICATIONS=true
VITE_ENABLE_OFFLINE_MODE=true

# Development Settings
VITE_DEBUG_MODE=false
VITE_SHOW_DEV_TOOLS=false

# Crisis Resources
VITE_CRISIS_HOTLINE=988
VITE_CRISIS_TEXT_LINE=741741

# Analytics (Optional)
VITE_GA_TRACKING_ID=your-google-analytics-id

# Environment
NODE_ENV=development

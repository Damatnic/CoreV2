{
  "name": "astral-core-react",
  "version": "1.0.0",
  "description": "An anonymous app for peer-to-peer mental-health support.",
  "scripts": {
    "dev": "netlify dev",
    "dev:vite": "vite",
    "clean": "rimraf dist",
    "build": "node scripts/create-test-build.js && npx workbox generateSW workbox-enhanced.js",
    "build:intelligent": "node scripts/create-test-build.js && npx workbox generateSW workbox-intelligent.js",
    "build:optimized": "node scripts/optimized-build.js",
    "build:critical-css": "node scripts/critical-css-optimizer.js",
    "build:analyze": "npm run build && node scripts/analyze-bundle.js",
    "build:mobile": "npm run optimize:videos && npm run build && npm run build:analyze",
    "build:production": "npm run clean && npm run build:optimized",
    "build:full": "npx vite build",
    "optimize:videos": "node scripts/optimize-videos.js",
    "analyze:tree-shaking": "node scripts/analyze-tree-shaking.js",
    "optimize:js": "npm run analyze:tree-shaking && echo 'Review tree-shaking-analysis.json for optimization opportunities'",
    "optimize:css": "npm run build:critical-css",
    "postbuild": "npm run verify:sw",
    "verify:sw": "node scripts/verify-service-worker.js",
    "build:netlify": "npm install && npm run build:production",
    "server": "node server-http.js",
    "server:dev": "nodemon server-http.js",
    "server:pm2": "pm2 start ecosystem.config.js",
    "server:build": "npm run build && npm run server",
    "test": "jest",
    "test:sw": "jest --testPathPattern=service-worker",
    "test:sw-unit": "jest tests/service-worker/registration.test.ts tests/service-worker/cache-strategies.test.ts",
    "test:sw-integration": "jest tests/service-worker/offline-functionality.test.ts tests/service-worker/crisis-scenarios.test.ts",
    "test:sw-performance": "jest tests/service-worker/performance.test.ts",
    "test:sw-cross-browser": "jest tests/service-worker/cross-browser.test.ts",
    "test:sw-coverage": "jest --testPathPattern=service-worker --coverage",
    "test:sw-watch": "jest --testPathPattern=service-worker --watch",
    "test:intelligent-caching": "node scripts/test-intelligent-caching.js performance",
    "test:e2e": "playwright test",
    "test:e2e-headed": "playwright test --headed",
    "test:e2e-debug": "playwright test --debug",
    "test:e2e-ui": "playwright test --ui",
    "test:e2e-crisis": "playwright test crisis-chat-flows.spec.ts emergency-escalation.spec.ts",
    "test:e2e-mobile": "playwright test mobile-responsive.spec.ts",
    "test:e2e-accessibility": "playwright test accessibility.spec.ts",
    "test:e2e-helper": "playwright test helper-certification.spec.ts",
    "test:e2e-safety": "playwright test safety-plan-creation.spec.ts",
    "test:e2e-report": "playwright show-report",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "preview": "vite preview"
  },
  "keywords": [
    "mental-health",
    "react",
    "typescript",
    "netlify"
  ],
  "author": "Astral Team",
  "license": "ISC",
  "dependencies": {
    "@google/genai": "^1.12.0",
    "@sentry/integrations": "^7.114.0",
    "@sentry/react": "^10.1.0",
    "@sentry/tracing": "^7.120.4",
    "@sentry/vite-plugin": "^4.0.2",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-core": "^4.22.0",
    "compression": "^1.8.1",
    "compromise": "^14.14.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "i18n-js": "^4.4.3",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "natural": "^8.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.6.1",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^7.7.1",
    "remark-gfm": "^4.0.0",
    "sentiment": "^5.0.2",
    "web-vitals": "^5.1.0",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.9.0",
    "@netlify/functions": "^2.8.2",
    "@playwright/test": "^1.54.2",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "buffer": "^6.0.3",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "eslint-plugin-storybook": "^0.8.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "netlify-cli": "^17.36.4",
    "playwright": "^1.54.2",
    "rimraf": "^5.0.10",
    "storybook": "^8.6.14",
    "stream-browserify": "^3.0.0",
    "terser": "^5.43.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3",
    "util": "^0.12.5",
    "vite": "^5.4.19",
    "workbox-build": "^7.3.0",
    "workbox-cli": "^7.3.0",
    "workbox-webpack-plugin": "^7.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}

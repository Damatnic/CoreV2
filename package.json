{
  "name": "astral-core-react",
  "version": "1.0.0",
  "description": "An anonymous app for peer-to-peer mental-health support.",
  "scripts": {
    "dev": "netlify dev",
    "dev:vite": "vite",
    "clean": "rimraf dist",
    "build": "node scripts/netlify-robust-build.js",
    "build:netlify-simple": "node scripts/netlify-build.js",
    "build:netlify-production": "node scripts/netlify-production-build.js",
    "build:netlify-safe": "node scripts/netlify-safe-build.js",
    "build:netlify-foolproof": "node scripts/netlify-foolproof-build.js",
    "build:intelligent": "node scripts/create-test-build.js && npx workbox generateSW workbox-intelligent.js",
    "build:optimized": "node scripts/optimized-build.js",
    "build:critical-css": "node scripts/critical-css-optimizer.js",
    "build:analyze": "npm run build && node scripts/analyze-bundle.js",
    "build:mobile": "npm run optimize:videos && npm run build && npm run build:analyze",
    "build:production": "npm run clean && npm run build:optimized",
    "build:full": "npx vite build",
    "optimize:videos": "node scripts/optimize-videos.js",
    "analyze:tree-shaking": "node scripts/analyze-tree-shaking.js",
    "optimize:js": "npm run analyze:tree-shaking && echo 'Review tree-shaking-analysis.json for optimization opportunities'",
    "optimize:css": "npm run build:critical-css",
    "postbuild": "npm run verify:sw",
    "verify:sw": "node scripts/verify-service-worker.js",
    "build:netlify": "node scripts/netlify-robust-build.js",
    "server": "node server-http.js",
    "server:dev": "nodemon server-http.js",
    "server:pm2": "pm2 start ecosystem.config.js",
    "server:build": "npm run build && npm run server",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "test:sw": "jest --testPathPattern=service-worker",
    "test:sw-unit": "jest tests/service-worker/registration.test.ts tests/service-worker/cache-strategies.test.ts",
    "test:sw-integration": "jest tests/service-worker/offline-functionality.test.ts tests/service-worker/crisis-scenarios.test.ts",
    "test:sw-performance": "jest tests/service-worker/performance.test.ts",
    "test:sw-cross-browser": "jest tests/service-worker/cross-browser.test.ts",
    "test:sw-coverage": "jest --testPathPattern=service-worker --coverage",
    "test:sw-watch": "jest --testPathPattern=service-worker --watch",
    "test:intelligent-caching": "node scripts/test-intelligent-caching.js performance",
    "test:e2e": "playwright test",
    "test:e2e-headed": "playwright test --headed",
    "test:e2e-debug": "playwright test --debug",
    "test:e2e-ui": "playwright test --ui",
    "test:e2e-crisis": "playwright test crisis-chat-flows.spec.ts emergency-escalation.spec.ts",
    "test:e2e-mobile": "playwright test mobile-responsive.spec.ts",
    "test:e2e-accessibility": "playwright test accessibility.spec.ts",
    "test:e2e-helper": "playwright test helper-certification.spec.ts",
    "test:e2e-safety": "playwright test safety-plan-creation.spec.ts",
    "test:e2e-report": "playwright show-report",
    "preview": "vite preview"
  },
  "keywords": [
    "mental-health",
    "react",
    "typescript",
    "netlify"
  ],
  "author": "Astral Team",
  "license": "ISC",
  "dependencies": {
    "@auth0/auth0-spa-js": "^2.3.0",
    "@sentry/integrations": "^7.114.0",
    "@sentry/react": "^10.1.0",
    "@sentry/vite-plugin": "^4.0.2",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-core": "^4.22.0",
    "dotenv": "^17.2.1",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.6.1",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^7.7.1",
    "remark-gfm": "^4.0.0",
    "pusher": "^5.2.0",
    "pusher-js": "^8.4.0",
    "web-vitals": "^5.1.0",
    "zod": "^4.0.17",
    "zustand": "^4.5.7",
    "vite": "^5.4.19",
    "@vitejs/plugin-react": "^4.3.3",
    "typescript": "^5.6.3",
    "terser": "^5.43.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^1.0.2",
    "workbox-cli": "^7.3.0",
    "workbox-build": "^7.3.0",
    "rollup": "^4.28.1",
    "@rollup/plugin-node-resolve": "^15.3.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-constant-elements": "^7.27.1",
    "@babel/plugin-transform-react-inline-elements": "^7.27.1",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@netlify/functions": "^2.8.2",
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitejs/plugin-legacy": "^5.4.3",
    "@vitejs/plugin-react-swc": "^4.0.0",
    "buffer": "^6.0.3",
    "chrome-launcher": "^1.2.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^12.8.1",
    "netlify-cli": "^17.36.4",
    "playwright": "^1.54.2",
    "rimraf": "^5.0.10",
    "stream-browserify": "^3.0.0",
    "ts-jest": "^29.4.1",
    "util": "^0.12.5"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.28.1",
    "@rollup/rollup-linux-x64-musl": "^4.28.1",
    "@rollup/rollup-linux-arm64-gnu": "^4.28.1",
    "@rollup/rollup-linux-arm64-musl": "^4.28.1",
    "@rollup/rollup-darwin-x64": "^4.28.1",
    "@rollup/rollup-darwin-arm64": "^4.28.1",
    "@rollup/rollup-win32-x64-msvc": "^4.28.1"
  },
  "engines": {
    "node": ">=20.9.0",
    "npm": ">=10.0.0"
  }
}